{
  "name": "artsflow-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port=5042",
    "dev:emulator": "NEXT_PUBLIC_EMULATOR=true next dev --port=5042",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\" && eslint --fix \"**/*.{js,ts,tsx}\"",
    "lint": "prettier --check \"**/*.{ts,tsx}\" && eslint \"**/*.{ts,tsx}\"",
    "ch": "yarn lint && yarn type-check",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "test:e2e": "start-server-and-test 'yarn dev' 3000 'yarn cypress'",
    "test:e2e:ci": "start-server-and-test 'yarn dev' 3000 'yarn cypress:headless'"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
